  },
  "homepage": "https://github.com/ukstv/is-core-module-es",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "prettier": "^2.5.1",
    "tsm": "^2.2.1",
    "typescript": "^4.6.2",
    "uvu": "^0.5.3",
